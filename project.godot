;   param=value ; assign values to parameters

config_version=5

[application]

config/name="08_watermelon"
run/main_scene="res://scene/main.tscn"
config/features=PackedStringArray("4.0", "Mobile")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=405
window/size/viewport_height=720
window/size/initial_position_type=2

[dotnet]

project/assembly_name="08_watermelon"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-idea-board/plugin.cfg")

[godot_idea_board]

directory_path="res://addons/godot-idea-board/savedata/"

[rendering]

renderer/rendering_method="mobile"
